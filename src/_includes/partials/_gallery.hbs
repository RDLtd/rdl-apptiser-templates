
<div class="image-gallery" aria-label="gallery">
    {{#each restaurant.imageGallery }}
        <a class="glightbox" href="https://res.cloudinary.com/rdl/image/upload/w_900,c_limit,q_auto,dpr_auto,f_auto/restaurants/{{ ../restaurant.id }}/{{ this }}">
            <img loading="lazy"
                 src="https://res.cloudinary.com/rdl/image/upload/w_600,h_400,c_fill,q_auto,dpr_auto,f_auto/restaurants/{{ ../restaurant.id }}/{{ this }}"
                 alt="{{ ../restaurant.name }} gallery image">
        </a>
    {{/each}}
</div>
