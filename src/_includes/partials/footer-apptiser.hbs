<!-- modal -->
<div class="modal-container">

    <!-- thanks -->
    <div class="booking-request-thanks">
        <img src="data:image/png;base64," alt="An email request has been sent"><span>Thanks, we'll be in touch.</span>
    </div>

    <!-- contact request -->
    <div id="modalRequest" class="claim-form">

        <form id="contactRequest">
            <h2>Join the collective and take back control of your data.</h2>
            <p>If you would like information about <strong>{{ demo.brand.name }}</strong> and how you can
               regain control of <strong>{{ demo.name }}'s</strong> valuable digital content, submit your
               contact details below:</p>
            <input type="hidden" name="id" value="{{ demo.id }}">
            <input type="hidden" name="brand" value="{{ demo.brand.prefix }}">
            <div class="text-field">
                <input id="full_name" type="text" name="name" placeholder=" " autofocus required>
                <label for="full_name">Your full name</label>
            </div>
            <!-- Email -->
            <div class="text-field">
                <input id="email" type="email" name="email" placeholder=" " required>
                <label for="email">Your Email Address</label>
            </div>
            <!-- Actions -->
            <div class="modal-actions">
                <button id="btnCancel" type="button" class="btn-cancel">CANCEL</button>
                <button type="submit" class="btn-submit">SEND REQUEST</button>
            </div>
        </form>
    </div>
</div>
