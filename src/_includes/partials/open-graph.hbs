<!-- open graph -->
<meta property="og:title"
      content="{{ dataSrc.name }}">
<meta property="og:type"
      content="website">
<meta property="og:url"
      content="{{ dataSrc.spwUrl }}">
<meta property="og:image"
      content="{{ config.image.url }}{{ config.image.transform.default }}{{ config.image.folder }}{{ dataSrc.id }}/{{ dataSrc.image.default }}">
