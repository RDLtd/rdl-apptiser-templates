<!-- open graph -->
<meta property="og:title"
      content="{{ data.name }}">
<meta property="og:type"
      content="website">
<meta property="og:url"
      content="{{ data.website }}">
<meta property="og:image"
      content="{{ config.image.url }}{{ config.image.transform.default }}{{ config.image.folder }}{{ data.id }}/{{ data.image.default }}">
