<!-- schema -->
<script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Restaurant",
        "name": "{{ demo.name }}",
        "@id": "{{ demo.spwUrl }}#id",
        "url": "{{ demo.spwUrl }}",
        "telephone": "{{ demo.phone }}",
        "servesCuisine": "{{ demo.cuisine }}",
        "acceptsReservations": "true",
        "image": "{{ demo.image.default }}",
        "address": {
            "@type": "PostalAddress",
            "streetAddress": "{{ demo.streetAddress }}",
            "addressLocality": "{{ demo.addressLocality }}",
            "postalCode": "{{ demo.postalCode }}",
            "addressCountry": "{{ demo.addressCountry }}"
        },
        "geo": {
            "@type": "GeoCoordinates",
            "latitude": "{{ demo.geocode.lat }}",
            "longitude": "{{ demo.geocode.lng }}"
        },
         "sameAs": "{{ demo.social.sameAs }}"
    }
</script>
