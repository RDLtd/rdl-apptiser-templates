<!-- Optional section tag start - Images -->
<div class="image-gallery" aria-label="gallery">
    <!-- {{ dataSrc.image.gallery }} -->
    {{#each dataSrc.image.gallery }}
        <a class="glightbox"
           href="{{ ../config.image.url }}{{ ../config.image.transform.gallery-display }}{{ ../config.image.folder }}{{ ../dataSrc.id }}/{{ this }}">
            <img loading="lazy"
                 src="{{ ../config.image.url }}{{ ../config.image.transform.gallery-thumb }}{{ ../config.image.folder }}{{ ../dataSrc.id }}/{{ this }}"
                 alt="{{ ../dataSrc.name }} gallery image">
        </a>
    {{/each}}
</div>
<!-- Optional section tag end - Images -->



